{
  "expo": {
    "name": "CoRide",
    "slug": "coride",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/CoRide-logos_black.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/CoRide-logos_black.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Your message here"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/CoRide-logos_black.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.fayyzzann.coride",
      "permissions": ["ACCESS_FINE_LOCATION", "ACCESS_COARSE_LOCATION"]
    },
    "web": {
      "favicon": "./assets/CoRide-logos_black.png"
    },
    "extra": {
      "eas": {
        "projectId": "55085298-bca5-45d7-a748-8c7807b99cc9"
      }
    },
    "owner": "fayyzzann"
  }
}
